<?xml version="1.0"?>
<package >
  <metadata>
    <id>Swagger.WebApiProxy</id>
    <version>0.1.21-alpha</version>
    <title>Swagger WebApi Proxy Generator</title>
    <description>Swagger.WebApiProxy is a T4 template that takes a Swagger 2.0 Spec and generators a proxy based on the WebAPI 2.0 Client Libraries</description>
    <authors>Phil Scott</authors>
    <owners>Phil Scott</owners>
    <projectUrl>https://github.com/enkafan/Swagger.WebApiProxy</projectUrl>
    <licenseUrl>https://raw.githubusercontent.com/enkafan/Swagger.WebApiProxy/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <tags>t4 webapi swagger</tags>
    <developmentDependency>true</developmentDependency>
    <dependencies>
      <dependency id="Microsoft.AspNet.WebApi.Client" version="5.2.2" />    
    </dependencies>
  </metadata>
  <files>
    <file src="..\src\Swagger.WebApiProxy.Template\SwaggerApiProxy.tt" target="content" />
    <file src="..\src\Swagger.WebApiProxy.Template\SwaggerApiProxy.tt.settings.xml" target="content" />
    <file src="..\src\Swagger.WebApiProxy.Template\BaseProxy.cs" target="content" />
  </files>
</package>